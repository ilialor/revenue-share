{
  "name": "revenue-share-lib",
  "version": "2.0.0",
  "description": "A flexible JavaScript library for calculating revenue sharing payouts with various distribution schemes, including Buy-to-Earn model",
  "main": "dist/revenue-share.js",
  "module": "dist/revenue-share.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.js\"",
    "prepare": "npm run build",
    "docs": "jsdoc -c jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/revenue-share-lib.git"
  },
  "keywords": [
    "revenue-sharing",
    "payout-calculation",
    "finance",
    "payments",
    "distribution",
    "buy-to-earn",
    "investment",
    "payback",
    "token-economy"
  ],
  "author": "aVa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/revenue-share-lib/issues"
  },
  "homepage": "https://github.com/yourusername/revenue-share-lib#readme",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "jsdoc": "^4.0.2",
    "prettier": "^2.8.4",
    "rollup": "^3.18.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
